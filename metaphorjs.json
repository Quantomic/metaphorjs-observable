{
    "version": "1.0.2",
    "push": [
        "github"
    ],
    "build": {
        "standalone": {
            "wrap": true,
            "global": true,
            "target": "dist/metaphorjs.observable.js",
            "files": [
                "../metaphorjs/src/MetaphorJs.js",
                "src/metaphorjs.observable.js"
            ],
            "expose": [
                [
                    "MetaphorJs.lib",
                    "Observable"
                ]
            ]
        },
        "npm": {
            "target": "dist/metaphorjs.observable.npm.js",
            "compile": false,
            "files": [
                [
                    "src/metaphorjs.observable.js",
                    {
                        "keepExports": true
                    }
                ]
            ]
        },
        "amd": {
            "target": "dist/metaphorjs.observable.amd.js",
            "files": [
                "src/metaphorjs.observable.js"
            ],
            "define": {
                "name": "metaphorjs-observable",
                "return": "Observable"
            }
        }
    }
}